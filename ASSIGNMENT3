   "source": [
    "def plusOne( digits):\n",
    "    size = len(digits)-1;\n",
    "    for num in range(size, -1, -1) :\n",
    "        if(num == 0):\n",
    "            if(digits[num]==9):\n",
    "                digits[num] = 0;\n",
    "                digits.insert(0,1);\n",
    "            else:\n",
    "                digits[num]  =digits[num] + 1;\n",
    "                break;\n",
    "        else:\n",
    "            if(digits[num]==9):\n",
    "                digits[num] = 0;\n",
    "            else:\n",
    "                digits[num]  =digits[num] + 1;\n",
    "                break;\n",
    "    return  digits;\n",
    "\n",
    "arr = [9, 9, 9, 9]\n",
    "print(plusOne(arr))"
   ]
