    "def is_power_of_three(n):\n",
    "    if n == 1:\n",
    "        return True\n",
    "    elif n < 1 or n % 3 != 0:\n",
    "        return False\n",
    "    else:\n",
    "        return is_power_of_three(n // 3)\n",
    "\n",
    "\n",
    "number = 27\n",
    "result = is_power_of_three(number)\n",
    "print(result)\n"
   ]
