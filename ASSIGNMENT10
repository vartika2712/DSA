   "source": [
    "def countSubstrings(s):\n",
    "    count = 0\n",
    "    for i in range(len(s)):\n",
    "        for j in range(i, len(s)):\n",
    "            if s[i] == s[j]:\n",
    "                count += 1\n",
    "    return count\n",
    "\n",
    "string = \"abcab\"\n",
    "count = countSubstrings(string)\n",
    "print(count)\n"
   ]
  },
