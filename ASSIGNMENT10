    "def calculateLength(s):\n",
    "    if s == '':\n",
    "        return 0\n",
    "    else:\n",
    "        return 1 + calculateLength(s[1:])\n",
    "\n",
    "string = \"Hello, World!\"\n",
    "length = calculateLength(string)\n",
    "print(length)\n"
   ]
