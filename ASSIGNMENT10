    "def printSubsets(s, curr=\"\", index=0):\n",
    "    if index == len(s):\n",
    "        print(curr)\n",
    "        return\n",
    "\n",
    "    printSubsets(s, curr + s[index], index + 1)\n",
    "\n",
    "    printSubsets(s, curr, index + 1)\n",
    "\n",
    "\n",
    "s = \"abc\"\n",
    "printSubsets(s)"
   ]
