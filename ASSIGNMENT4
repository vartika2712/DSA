    "def shuffle(nums, n):\n",
    "    # a = []\n",
    "    # size=len(nums)\n",
    "    # for i in range(n,size):\n",
    "    #     a.append(nums[i])\n",
    "    #     nums.remove(nums[i])\n",
    "    # print(nums)\n",
    "    # print(a)\n",
    "    arr1=[]\n",
    "    arr2=[]\n",
    "    arr3=[]\n",
    "    for i in range(n):\n",
    "        arr1.append(nums[i])\n",
    "    for i in range(n,2*n):\n",
    "        arr2.append(nums[i])\n",
    "    for i in range(n):\n",
    "        arr3.append(arr1[i])\n",
    "        arr3.append(arr2[i])\n",
    "    return arr3\n",
    "\n",
    "\n",
    "\n",
    "arr = [2,3,5,4,1,7]\n",
    "print(shuffle(arr,3))"
   ]
  }
