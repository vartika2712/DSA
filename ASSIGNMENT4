   "source": [
    "def transpose(matrix):\n",
    "    result = [[matrix[i][j] for i in range(len(matrix))] for j in range(len(matrix[0]))]\n",
    "    return result\n",
    "arr = [[2,4,-1], [-10,5,11], [18,-7,6]]\n",
    "print(transpose(arr))"
   ]
  },
