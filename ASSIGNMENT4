   "source": [
    "def sortedSquares( nums):\n",
    "    for i in range(len(nums)):\n",
    "        nums[i]*=nums[i]\n",
    "    nums.sort()\n",
    "    return nums\n",
    "\n",
    "arr= [-4,-1,0,3,10]\n",
    "print(sortedSquares(arr))"
   ]
  },
