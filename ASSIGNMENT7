   "source": [
    "def reverseWords(s):\n",
    "    words = s.split()\n",
    "    reversed_words = []\n",
    "\n",
    "    for word in words:\n",
    "        reversed_word = word[::-1]\n",
    "        reversed_words.append(reversed_word)\n",
    "\n",
    "    result = ' '.join(reversed_words)\n",
    "    return result\n",
    "\n",
    "\n",
    "s = \"Let's code and have fun!\"\n",
    "result = reverseWords(s)\n",
    "print(result)\n"
   ]
  },
