    "def firstUniqChar(s):\n",
    "    char_freq = {}\n",
    "\n",
    "   \n",
    "    for char in s:\n",
    "        char_freq[char] = char_freq.get(char, 0) + 1\n",
    "\n",
    "   \n",
    "    for i in range(len(s)):\n",
    "        if char_freq[s[i]] == 1:\n",
    "            return i\n",
    "\n",
    "    return -1\n",
    "\n",
    "\n",
    "\n",
    "s = \"loveleetcode\"\n",
    "result = firstUniqChar(s)\n",
    "print(\"First non-repeating character index:\", result)\n"
   ]
