   "source": [
    "from collections import deque\n",
    "import heapq\n",
    "\n",
    "def deckRevealedIncreasing(deck):\n",
    "    deck.sort()  \n",
    "    queue = deque(range(len(deck)))  \n",
    "    \n",
    "    result = [0] * len(deck)\n",
    "    for card in deck:\n",
    "        result[queue.popleft()] = card  \n",
    "        if queue: \n",
    "            queue.append(queue.popleft())\n",
    "    \n",
    "    return result\n",
    "deck = [1,1000]\n",
    "ordering = deckRevealedIncreasing(deck)\n",
    "print(ordering)\n"
   ]
